{
  "name": "uppi.fi",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "dev:frontend": "vite --config frontend/vite.config.ts --host",
    "dev:backend": "tsnd -r tsconfig-paths/register --quiet --respawn backend/src/index.ts",
    "dev:bot": "tsnd  -r tsconfig-paths/register --quiet --respawn bot/src/index.ts",
    "build:frontend": "vite build frontend --emptyOutDir --outDir ../dist/frontend",
    "build:backend": "tsc -p tsconfig.backend.json && tscpaths -p tsconfig.backend.json -s .",
    "build:bot": "tsc -p tsconfig.bot.json && tscpaths -p tsconfig.bot.json -s .",
    "start:backend": "node dist/backend/src/index.js",
    "start:bot": "node dist/bot/src/index.js",
    "migrate": "graphile-migrate migrate",
    "migrate:watch": "graphile-migrate watch",
    "migrate:commit": "graphile-migrate commit",
    "migrate:uncommit": "graphile-migrate uncommit",
    "lint:eslint": "eslint '**/*.{js,ts,tsx}' --quiet --fix",
    "lint:prettier": "prettier --write .",
    "generate-types": "kanel",
    "dev": "concurrently \"npm:dev:frontend\" \"npm:dev:backend\" \"npm:dev:bot\" \"npm:migrate:watch\" -c \"green,yellow,red,magenta\" --names \"frontend,backend.,bot.....,migrate.\"",
    "build": "rimraf dist && concurrently \"npm:build:frontend\" \"npm:build:backend\" \"npm:build:bot\"  -c \"green,yellow,red\" --names \"frontend,backend.,bot.....\"",
    "start": "concurrently \"npm:start:backend\" \"npm:start:bot\"  -c \"green,yellow,red\" --names \"backend.,bot.....\"",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook -o dist/storybook"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "npm run lint:prettier",
      "npm run lint:eslint"
    ]
  },
  "dependencies": {
    "@ffmpeg-installer/ffmpeg": "^1.1.0",
    "@ffprobe-installer/ffprobe": "^1.2.0",
    "@hookform/error-message": "^2.0.0",
    "@zattoo/use-double-click": "^1.1.0",
    "axios": "^0.22.0",
    "classnames": "^2.3.1",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "envalid": "6.0.2",
    "express": "^4.17.1",
    "fluent-ffmpeg": "^2.1.2",
    "jsonwebtoken": "^8.5.1",
    "kanel": "^2.3.2",
    "multer": "^1.4.3",
    "nanoid": "^3.1.28",
    "passport": "^0.5.0",
    "passport-jwt": "^4.0.0",
    "pg-promise": "^10.11.0",
    "pluralize": "^8.0.0",
    "rc-tooltip": "^5.1.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-hook-form": "^7.18.0",
    "react-player": "^2.9.0",
    "react-use": "^17.3.1",
    "recoil": "^0.4.1",
    "serve": "^12.0.1",
    "telegraf": "^4.4.2",
    "typescript": "^4.4.3",
    "uuid": "^8.3.2",
    "vite": "^2.6.2",
    "wouter": "^2.8.0-alpha.1"
  },
  "devDependencies": {
    "@babel/core": "^7.15.8",
    "@iconify/react": "^3.0.1",
    "@storybook/addon-actions": "^6.4.0-beta.12",
    "@storybook/addon-essentials": "^6.4.0-beta.12",
    "@storybook/addon-links": "^6.4.0-beta.12",
    "@storybook/preset-scss": "^1.0.3",
    "@storybook/react": "^6.4.0-beta.12",
    "@types/classnames": "^2.3.1",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/fluent-ffmpeg": "^2.1.18",
    "@types/lodash-es": "^4.17.5",
    "@types/multer": "^1.4.7",
    "@types/passport": "^1.0.7",
    "@types/passport-jwt": "^3.0.6",
    "@types/react": "^17.0.26",
    "@types/react-dom": "^17.0.9",
    "@types/uuid": "^8.3.1",
    "@typescript-eslint/eslint-plugin": "^4.32.0",
    "@typescript-eslint/parser": "^4.32.0",
    "@vitejs/plugin-react": "^1.0.1",
    "babel-loader": "^8.2.2",
    "concurrently": "^6.3.0",
    "css-loader": "5.2.6",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-react": "^7.26.1",
    "eslint": "^7.32.0",
    "graphile-migrate": "^1.2.0",
    "husky": "^7.0.2",
    "lint-staged": "^11.1.2",
    "lodash-es": "^4.17.21",
    "prettier": "^2.4.1",
    "rimraf": "^3.0.2",
    "sass-loader": "10.1.1",
    "sass": "^1.43.4",
    "style-loader": "2.0.0",
    "ts-node-dev": "^1.1.8",
    "tsconfig-paths-webpack-plugin": "^3.5.1",
    "tsconfig-paths": "^3.11.0",
    "tscpaths": "^0.0.9"
  }
}
